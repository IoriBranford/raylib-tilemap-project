[submodule "lib/libxml2"]
	path = lib/libxml2
	url = https://gitlab.gnome.org/GNOME/libxml2.git
[submodule "lib/chipmunk2d"]
	path = lib/chipmunk2d
	url = https://github.com/slembcke/Chipmunk2D.git
