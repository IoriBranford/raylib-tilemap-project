{
    "dotnet.defaultSolution": "disable",
    "files.associations": {
        "*.lua": "lua",
        "*.ldtk": "json",
        "*.h": "c",
        "*.c": "c",
        "*.cpp": "cpp"
    },
    "json.schemas": [
        {
            "fileMatch": ["*.ldtk"],
            "url": "https://ldtk.io/files/JSON_SCHEMA.json"
        }
    ],
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "Lua.runtime.version": "Lua 5.1"
}